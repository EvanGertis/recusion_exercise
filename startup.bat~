echo Running startup
set DATABASE_URL=jdbc:mysql://localhost:3306/TestExercise?serverTimezone=UTC
set DATABASE_USER=root
set DATABASE_PASSWORD=test123
set SCHEMA=TestExercise
echo The following environment variables have been set
Get-ChildItem Env: |findstr DATABASE_URL
Get-ChildItem Env: |findstr DATABASE_USER
Get-ChildItem Env: |findstr DATABASE_PASSWORD
Get-ChildItem Env: |findstr SCHEMA
